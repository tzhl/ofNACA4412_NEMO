FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "include/initialConditions"

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform $turbulentKE;

boundaryField
{

    inlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;

    }
    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    NEMOWing
    {
        type            kqRWallFunction;
        value           0;
    }
    symmetryPlane1
    {
        type            symmetryPlane;
    }
}


